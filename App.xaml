<Application x:Class="Lab3.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Lab3"
             xmlns:localization="clr-namespace:Lab3.Localization"
             xmlns:sorting="clr-namespace:Lab3.Sorting"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ObjectDataProvider x:Key="Strings" ObjectType="{x:Type localization:CultureResources}" MethodName="GetStringsInstance" />
        <sorting:EnumBooleanConverter x:Key="EnumBooleanConverter" />
    </Application.Resources>
</Application>
